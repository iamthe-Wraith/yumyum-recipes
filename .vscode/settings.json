{
  "eslint.validate": [
    "javascript",
    "typescript",
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
  },
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "cSpell.words": [
    "headlessui",
    "kelevens",
    "linebreak",
    "rgossiaux",
    "yumyum"
  ],
}